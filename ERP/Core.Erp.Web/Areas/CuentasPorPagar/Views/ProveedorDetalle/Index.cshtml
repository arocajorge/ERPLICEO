
@{
    ViewBag.Title = "Cuentas por proveedor";
    if (string.IsNullOrEmpty(Core.Erp.Web.Helps.SessionFixed.IdUsuario) || string.IsNullOrEmpty(Core.Erp.Web.Helps.SessionFixed.IdEmpresa))
    {
        Response.Redirect("~/Account/Login");
    }
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm())
{
    @Html.Action("GridViewPartial_Cuentas_x_Proveedor", new { IdProveedor = ViewBag.IdProveedor })
}

<div class="box-footer">
    <a class="btn btn-default" href="@Url.Action("Index","Proveedor",new { Area = "CuentasPorPagar" },null)">Volver</a>
</div>